javascript: function skipAds() {                const video = document.querySelector('video');                              const skipAdsButton = document.querySelector('.ytp-ad-skip-button');                const smallAdsButton = document.querySelector('.ytp-ad-overlay-close-button');                const noSkipAdsButton = document.querySelector('.ytp-ad-preview-text');                              if (skipAdsButton) {                  video.playbackRate = video.playbackRate != 1 ? 1 : 10;                                setTimeout(() => {                    console.log(%27add skipped%27);                  }, 500);                                skipAdsButton.click();                } else if (smallAdsButton) {                  smallAdsButton.click();                  console.log(%27small popup add skipped%27);                } else if (noSkipAdsButton) {                  video.playbackRate = video.playbackRate != 1 ? 1 : 10;                  console.log(%27add played fast%27);                }              }                            (function () {                console.log(%27Youtube ads skipper is loaded%27);                              document.onkeyup = function (event) {                  const video = document.querySelector(%27video%27);                  let e = event || window.event;                                if (e.ctrlKey) {                    skipAds();                  }                                if (e.key === %27+%27) {                    video.playbackRate += 0.25;                    console.log(%27Youtube video speed is: %27 + video.playbackRate);                  }                                if (e.key === %27-%27 && video.playbackRate > 0.25) {                    video.playbackRate -= 0.25;                    console.log(%27Youtube video speed is: %27 + video.playbackRate);                  }                                if (e.key === %270%27 && e.ctrlKey) {                    video.playbackRate = 1;                    console.log(%27Youtube video speed is: %27 + video.playbackRate);                  }                };                              setInterval(skipAds, 500);              })();